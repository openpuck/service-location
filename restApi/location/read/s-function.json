{
  "name": "read",
  "customName": false,
  "customRole": false,
  "handler": "location/read/handler.handler",
  "timeout": 3,
  "memorySize": 128,
  "custom": {
    "excludePatterns": [],
    "envVars": []
  },
  "endpoints": [
    {
      "path": "location/{id}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiGatewayRequestTemplate}",
      "responses": {
        "400": {
          "statusCode": "400",
          "selectionPattern": "BadRequestException.*",
          "responseTemplates": "$${apiGatewayResponseErrorTemplate}"
        },
        "404": {
          "selectionPattern": "NotFoundException.*",
          "statusCode": "404",
          "responseTemplates": "$${apiGatewayResponseErrorTemplate}",
          "responseParameters": {
            "method.response.header.errorMessage": "integration.response.body.errorMessage"
          }
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": []
}
